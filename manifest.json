{
  "name": "compact Overleaf",
  "description": "compact your overleaf to focus on editing!",
  "version": "1.0.3",
  "homepage_url": "https://github.com/modaresimr/compact_overleaf",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"],
  "browser_action": {},
  "icons": {
    "128": "/logo-128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": ["/thirdparty/jquery.min.js", "/thirdparty/ResizeSensor.js", "/thirdparty/ElementQueries.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
